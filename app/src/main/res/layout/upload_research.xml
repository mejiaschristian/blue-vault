<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/upload_research">

    <Button
        android:id="@+id/button7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:alpha="0"
        android:text="Button"
        tools:layout_editor_absoluteX="311dp"
        tools:layout_editor_absoluteY="25dp" />

    <EditText
        android:id="@+id/editTextText"
        android:layout_width="390dp"
        android:layout_height="46dp"
        android:ems="10"
        android:hint="Enter your research title"
        android:inputType="text"
        android:singleLine="false"
        tools:layout_editor_absoluteX="10dp"
        tools:layout_editor_absoluteY="164dp" />

    <EditText
        android:id="@+id/editTextText3"
        android:layout_width="387dp"
        android:layout_height="44dp"
        android:ems="10"
        android:hint="Name of the authors..."
        android:inputType="text"
        android:singleLine="false"
        tools:layout_editor_absoluteX="12dp"
        tools:layout_editor_absoluteY="255dp" />

    <EditText
        android:id="@+id/editTextText4"
        android:layout_width="373dp"
        android:layout_height="40dp"
        android:ems="10"
        android:hint="Enter your abstract"
        android:inputType="text"
        android:singleLine="false"
        tools:layout_editor_absoluteX="19dp"
        tools:layout_editor_absoluteY="429dp" />

    <EditText
        android:id="@+id/editTextText5"
        android:layout_width="377dp"
        android:layout_height="41dp"
        android:ems="10"
        android:hint="Add tags..."
        android:inputType="text"
        tools:layout_editor_absoluteX="14dp"
        tools:layout_editor_absoluteY="535dp" />

    <EditText
        android:id="@+id/editTextText6"
        android:layout_width="377dp"
        android:layout_height="41dp"
        android:ems="10"
        android:hint="Enter your research link..."
        android:inputType="text"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="619dp" />

    <Button
        android:id="@+id/button8"
        android:layout_width="110dp"
        android:layout_height="56dp"
        android:alpha="0"
        android:text="Button"
        tools:layout_editor_absoluteX="57dp"
        tools:layout_editor_absoluteY="678dp" />

    <Button
        android:id="@+id/button9"
        android:layout_width="112dp"
        android:layout_height="53dp"
        android:alpha="0"
        android:text="Button"
        tools:layout_editor_absoluteX="261dp"
        tools:layout_editor_absoluteY="678dp" />

</androidx.constraintlayout.widget.ConstraintLayout>